<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/select"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="222dp" />

    <CheckBox
        android:id="@+id/checkBoxSemiDetachedHome"
        android:layout_width="220dp"
        android:layout_height="49dp"
        android:layout_marginStart="48dp"
        android:text="@string/semi_detached_home"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="305dp" />

    <CheckBox
        android:id="@+id/checkBoxApartment"
        android:layout_width="222dp"
        android:layout_height="43dp"
        android:layout_marginStart="48dp"
        android:text="@string/apartment"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="354dp" />

    <CheckBox
        android:id="@+id/checkBoxTownHouse"
        android:layout_width="222dp"
        android:layout_height="43dp"
        android:layout_marginStart="48dp"
        android:text="@string/town_house"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="446dp" />

    <CheckBox
        android:id="@+id/checkBoxCondo"
        android:layout_width="222dp"
        android:layout_height="49dp"
        android:layout_marginStart="48dp"
        android:text="@string/condo"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="397dp" />

    <CheckBox
        android:id="@+id/checkBoxDetachedHome"
        android:layout_width="222dp"
        android:layout_height="43dp"
        android:layout_marginStart="48dp"
        android:text="@string/detached_home"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="262dp" />

    <Button
        android:id="@+id/buttonSubmit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/submit"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="539dp" />

</androidx.constraintlayout.widget.ConstraintLayout>